{
    "version": 2,
    "builds": [
      {
        "src": "public/*",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.js"
      }
    ]
  }